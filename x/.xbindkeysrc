# Increase volume
"pamixer --increase 5; notify-send 'pamixer --get-volume' -t 200"
	XF86AudioRaiseVolume

# Decrease volume
"pamixer --decrease 5; notify-send 'pamixer --get-volume' -t 200"
	XF86AudioLowerVolume

# Mute volume
"pamixer --toggle-mute; notify-send 'pamixer --get-mute' -t 200"
	XF86AudioMute

